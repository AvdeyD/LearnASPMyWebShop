@using CinemaC.Models
@model CinemaC.Models.Domain.TimeSlot[]


@section SubNav
{
    @Html.Partial("SubNavigation", SubNavigationMenu.TimeSlots)
}


<table>
    <tr>
        <th>Id</th>
        <th>Hall</th>
        <th>Movie</th>
        <th>StartTime</th>
        <th>Cost</th>
        <th>Format</th>
        <th>Action</th>
    </tr>
    @foreach (var timeSlot in Model)
    {
        <tr>
            <td>@timeSlot.Id</td>
            <td>@timeSlot.HallId</td>
            <td>@timeSlot.MovieId</td>
            <td>@timeSlot.StarTime</td>
            <td>@timeSlot.Cost</td>
            <td>@timeSlot.Format</td>
            <td> @Html.ActionLink("Edit", "EditTimeSlot", "Admin", new { timeslotId = timeSlot.Id }, null)</td>
        </tr>
    }


</table>

